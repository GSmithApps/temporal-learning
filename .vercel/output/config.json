{
  "version": 3,
  "routes": [
    {
      "src": "^/[^./]+\\.[0-9a-f]{8}\\.(css|js)$",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/assets/images/[^/]+-[0-9a-f]{32}\\.(ico|svg|jpg|jpeg|png|gif|webp)$",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/assets/medias/[^/]+-[0-9a-f]{32}\\.(ogv|wav|mp3|m4a|aac|oga|flac)$",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/assets/files/[^/]+-[0-9a-f]{32}\\.(pdf|doc|docx|xls|xlsx|zip|rar)$",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/ideal-img/[^/]+\\.[0-9a-f]{7}\\.\\d+\\.(png|jpe?g|gif)$",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/\\.well-known(?:/.*)?$"
    },
    {
      "src": "^/((?:[^/]+/)*[^/\\.]+)$",
      "headers": {
        "Location": "/$1/"
      },
      "status": 308
    },
    {
      "src": "^/((?:[^/]+/)*[^/]+\\.\\w+)/$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": ".*",
      "status": 404,
      "dest": "404.html"
    }
  ],
  "framework": {
    "version": "2.4.3"
  },
  "crons": [],
  "flags": []
}
